        <seg>Getting Started</seg>
      </tuv>
      <tuv lang="pt-BR" changeid="LibreOfficeBR" changedate="20241028T203217Z" creationid="LibreOfficeBR" creationdate="20241028T203217Z">
        <seg>Iniciar</seg>
      </tuv>
    </tu>
    <tu>
